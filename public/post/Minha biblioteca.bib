@article{conceicao_aparecida_vedovello_revisao_2006,
	title = {Revisão crítica às abordagens a parques tecnológicos: alternativas interpretativas às experiências brasileiras recentes},
	volume = {3},
	issn = {1809-2039},
	url = {http://www.revistas.usp.br/rai/article/view/79066},
	doi = {http://dx.doi.org/10.5585/rai.v3i2.58},
	abstract = {Parques tecnológicos têm sido reconhecidos como instrumentos de integração de múltiplos
atores, instituições e atividades relacionadas aos processos de inovação tecnológica. Em
diferentes contextos, várias experiências têm sido internacionalmente implantadas. Em
comum, teriam o fato de constituírem loci privilegiados compostos por infra-estruturas físicas
e organizacionais de articulação e de criação de conhecimentos componentes dos processos de
inovação, a saber: científico-tecnológicos (universidades); aplicados (empresas/mercado);
organizacionais (práticas/relacionamentos). O artigo procede a uma revisão crítica a essa
literatura internacional, distinguindo duas fases: perspectivas históricas (1960 a meados 1990)
e rupturas contemporâneas (meados 1990 até atualidade). Com visão ex-post e baseado em
pesquisa empírica (11 parques em implantação no país), esse trabalho busca contribuir para a
discussão crítica das experiências emergentes no Brasil. De partida, estabelece-se a seguinte
questão: como as experiências recentes de implantação de parques tecnológicos contribuiriam à consolidação da inovação tecnológica no Brasil? Subjacente a essa reflexão estaria a percepção de que os parques representariam projetos que trariam a promessa de construção de futuro, por meio da revitalização econômica e desenvolvimento de longo prazo.},
	urldate = {2017-04-24},
	journal = {Revista de Administração e Inovação},
	author = {{Conceição Aparecida Vedovello} and {Valéria Maria Martins Judice} and {Anne-Marie Dalaunay Maculan}},
	year = {2006},
	pages = {103 -- 118}
}

